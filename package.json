{
  "name": "poligon",
  "version": "1.0.0",
  "description": "React.js web application template with Express.js backend",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "dev-client": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "clean": "rd /s /q dist 2>nul || echo Dist folder cleared",
    "lint": "echo Linting not configured yet",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "echo \"Unit tests not configured yet\" && exit 1",
    "test:integration": "echo \"Integration tests not configured yet\" && exit 1",
    "test:api": "echo \"API tests not configured yet\" && exit 1",
    "test:e2e": "echo \"E2E tests not configured yet\" && exit 1",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "knex migrate:make",
    "db:migrate:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "db:seed:make": "knex seed:make",
    "logs": "type stderr.log 2>nul || echo No logs found",
    "logs:clear": "del /f stderr.log 2>nul || echo Logs cleared"
  },
  "repository": {
    "type": "git",
